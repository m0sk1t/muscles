@import 'jeet'
@import 'rupture'

.ya-inner-grid-1-1
    col()
.ya-inner-grid-1-2
    col(1/2, cycle: 2)
    +below(767px)
        stack()
.ya-inner-grid-1-3
    col(1/3, cycle: 3)
    +below(767px)
        stack()
.ya-inner-grid-1-4
    col(1/4, cycle: 4)
    +below(959px)
        col(1/2, uncyle: 4, cycle: 2)
    +below(767px)
        stack()
.ya-inner-grid-1-5
    col(1/5, cycle: 5)
    +below(959px)
        stack()
